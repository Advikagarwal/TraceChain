{
  "name": "agritrust-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "blockchain:compile": "cd blockchain && npm run compile",
    "blockchain:test": "cd blockchain && npm run test",
    "blockchain:deploy": "cd blockchain && npm run deploy:local",
    "backend:dev": "cd backend && python main.py",
    "backend:test": "cd backend && pytest",
    "dev:all": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "setup:blockchain": "cd blockchain && npm install && npm run compile"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.8.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "ethers": "^6.0.0",
    "framer-motion": "^10.16.5",
    "jsonwebtoken": "^9.0.2",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.0.0",
    "vitest": "^3.2.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^20.9.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.6.1"
  }
}
